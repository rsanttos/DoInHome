      <Label layoutX="14.0" layoutY="14.0" text="Tarefas do Dia">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tvTarefasDoDia" layoutX="14.0" layoutY="64.0" prefHeight="270.0" prefWidth="521.0">
        <columns>
          <TableColumn fx:id="tcTarefaDescricao" prefWidth="315.0" text="Descrição" />
          <TableColumn fx:id="tcTarefaUsuario" minWidth="0.0" prefWidth="106.0" text="Responsável" />
            <TableColumn fx:id="tcTarefaDataFim" prefWidth="99.0" text="Data Fim" />
        </columns>
      </TableView>
      <Button fx:id="btTarefaNovaTarefa" layoutX="199.0" layoutY="19.0" mnemonicParsing="false" text="Nova tarefa" />
   </children>
</AnchorPane>
